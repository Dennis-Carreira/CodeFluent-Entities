<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap-responsive.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="container">
        <!-- Navbar ================================================== -->
        <div class="navbar navbar-inverse">
            <div class="navbar-inner">
                <div class="container">
                    <a class="brand" href="./Home">SoftFluent.Samples.GED</a>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="span12">
                <fieldset>
                    <legend>@ViewBag.Message</legend>
                </fieldset>
                @RenderBody()
            </div>
        </div>
    </div>
    <script type="text/javascript">
        $(function () {

        });
    </script>
</body>
</html>
